[application]

config/name="LeberOlympiade2023"
run/main_scene="res://assets/StartScreen/StartScreen.tscn"
config/icon="res://icon.png"

[autoload]

Stats="*res://assets/Stats/Stats.tscn"
AudioManager="*res://assets/AudioManager/AudioManager.tscn"
GameEvents="*res://assets/GameEvents/GameEvents.tscn"

[display]

window/size/width=320
window/size/height=180
window/size/resizable=false
window/size/always_on_top=true
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

